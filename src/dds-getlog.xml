<refentry xmlns="http://docbook.org/ns/docbook" version="5.0"
	  xml:id="dds-getlog">
        <refmeta>
            <refentrytitle>dds-getlog</refentrytitle>
            <manvolnum>1</manvolnum>
        </refmeta>
	<refnamediv>
		<refname>dds-getlog</refname>
		<refpurpose>download log files from workers</refpurpose>
		<refclass>UNIX/Linux/OSX</refclass>
	</refnamediv>
	<refsynopsisdiv>
		<cmdsynopsis>
			<command>dds-getlog</command>
			<group choice="req">
				<arg>--all,-a</arg>
			</group>
		</cmdsynopsis>
	</refsynopsisdiv>
	<refsect1>
		<title>Description</title>
		<para>This command allows to download log files from worker nodes.
		</para>
	</refsect1>
	<refsect1>
		<title>Options</title>
		<variablelist>
			<varlistentry>
				<term><option>--all,-a</option></term>
				<listitem>
					<para>Download log files from all active workers.</para>
				</listitem>
			</varlistentry>
		</variablelist>
	</refsect1>
</refentry>
